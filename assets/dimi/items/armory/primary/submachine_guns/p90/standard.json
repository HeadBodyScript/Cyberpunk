{
    "model": {
      "type": "minecraft:condition",
      "property": "minecraft:keybind_down",
      "keybind": "key.sneak",
      "on_true": {
        "type": "minecraft:select",
        "property": "minecraft:custom_model_data",
        "cases": [
          {
            "when": "lotus",
            "model": {
              "type": "minecraft:model",
              "model": "dimi:item/armory/primary/submachine_guns/p90/standard_ads_lotus"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "dimi:item/armory/primary/submachine_guns/p90/standard_ads"
        }
      },
      "on_false": {
        "type": "minecraft:select",
        "property": "minecraft:custom_model_data",
        "cases": [
          {
            "when": "lotus",
            "model": {
              "type": "minecraft:model",
              "model": "dimi:item/armory/primary/submachine_guns/p90/standard_lotus"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:model",
          "model": "dimi:item/armory/primary/submachine_guns/p90/standard"
        }
      }
    }
  }